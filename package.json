{
  "name": "kenpo-watcher",
  "version": "1.2.0",
  "scripts": {
    "bundle": "esbuild index.js --bundle --platform=node --outdir=dist --external:filePath --external:t2.extends --external:path2",
    "build-binary": "pkg dist/index.js --targets node16-linux-x64 --output kenpo-watcher",
    "start": "bash -c 'xvfb-run --server-args=\"-screen 0 1024x768x24\" node server.js'"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "express": "^4.18.2",
    "openai": "^4.3.1",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "follow-redirects": "^1.15.9",
    "form-data": "^4.0.3",
    "proxy-from-env": "^1.1.0",
    "random-useragent": "^0.5.0",
    "esbuild": "^0.19.12",
    "pkg": "^5.8.1"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
